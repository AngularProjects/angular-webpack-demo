  "description": "Example app with angular and webpack",
  "scripts": {
    "build": "rimraf ./client/dist/build && webpack --bail --progress --profile",
    "serve": "webpack-dev-server --history-api-fallback --inline --progress",
    "start": "babel-node server/app.js",
    "prod": "rimraf ./client/dist/build && webpack --bail --progress --profile && set NODE_ENV=production&& babel-node server/app.js",
    "dev": "nodemon server/app.js --exec babel-node"
